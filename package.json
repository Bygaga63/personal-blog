{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\""
  },
  "dependencies": {
    "@remix-run/node": "^1.6.2",
    "@remix-run/react": "^1.6.2",
    "@remix-run/serve": "^1.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-highlight": "^5.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.2",
    "@remix-run/eslint-config": "^1.6.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
